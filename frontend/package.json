{
  "name": "ai-ecommerce-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "start": "vite --mode development",
    "build": "tsc && vite build --mode production",
    "build:dev": "tsc && vite build --mode development",
    "build:analyze": "tsc && vite build --mode production && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint . --no-eslintrc -c .eslintrc.cjs --ext ts,tsx --report-unused-disable-directives --max-warnings 1000",
    "lint:fix": "eslint . --no-eslintrc -c .eslintrc.cjs --ext ts,tsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@mui/x-data-grid": "^6.18.0",
    "@mui/x-date-pickers": "^8.15.0",
    "@stomp/stompjs": "^7.1.1",
    "@tanstack/react-query": "^5.17.0",
    "@types/nprogress": "^0.2.3",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "date-fns": "^3.0.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^6.8.0",
    "recharts": "^3.4.1",
    "sockjs-client": "^1.6.1",
    "terser": "^5.44.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.17.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/sockjs-client": "^1.5.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  }
}
